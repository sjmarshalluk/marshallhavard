<div class="wrapper">

	<div class="row">
		<div class="c4 s4">
			<div id="logo-medium">
				<%= link_to "Marshall Havard", root_path %>
			</div>

			<%= simple_form_for :session, url: session_path, method: :post do |f| %>

				<%= f.input :email, label: "Email address" %>
				<%= f.input :password, label: "Password" %>

				<%= f.button :submit, "Log in" %>

			<% end %>

		</div>
	</div>
</div>