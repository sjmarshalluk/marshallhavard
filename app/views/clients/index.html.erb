


	<section id="advice" class="dark padded">
		<div class="wrapper push-top">
		<div class="row no-margin">
			<div class="c8 client-list">
				<% @clients.each do |client| %> 
						<h2>	
							<%= link_to "#{client.title}", client_path(client) %>
	
						</h2>
					<% end %>
				
			</div>
			<div class="c3 s1">
				<p class="small">We can help in all of these exciting ways. If you have any questions, please do not hesitate to get in touch.</p>
		</div>
		
		</div>
	
	</section>

	<% if logged_in? %>
		<div class="admin-tools center small-copy push-top faded">
			<%= link_to "Add a client type", new_client_path %>
		</div>
	<% end %>





	
